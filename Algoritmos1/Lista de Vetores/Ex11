algoritmo ExibeNumerosDoVetorNaOrdemInversa
var
	i, n: inteiro
	A: vetor[1..111] de real
	inverso: string
início
	escreva("Digite um número inteiro entre 1 e 111")
	leia(n)
	enquanto i < 1 ou n > 111 faça
		escreva("Entrada inválida")
		leia(n)
	fim enquanto
	para i de 1 até n faça
		leia(A[i])
	fim para
	para i de 1 até n faça
		escreva(A[n-i+1])
	fim para
fim algoritmo
